        <!-- buttons -->
        <invoker type="formActions" caption="Supersede"  action="supersede" index="0" visibleWhen="#{ entity.state == 2 }"/>    
        <invoker type="formActions" caption="Close Issue"  action="closeIssue" index="1" visibleWhen="#{ entity.state == 2 }"/>    
        <invoker type="formActions" caption="View History"  action="viewHist" index="2" visibleWhen="#{ entity.state == 2 }"/>    
            
    </invokers>
