<workunit>
    <invokers>
        <invoker type="building_permit_requirement:open" caption="Requirement Finding" action="open" index="1" target="popup"/>    
        
        <!-- buttons -->
        <invoker type="formActions" caption="Accept"  action="accept" index="0" visibleWhen="#{ actionable == true }" />    
        <invoker type="formActions" caption="For Revision"  action="revise" index="1" visibleWhen="#{ actionable == true }"/>    
        <invoker type="formActions" caption="Not Applicable"  action="na" index="2" visibleWhen="#{ actionable == true }"/>    
        
        <invoker type="formActions" caption="Edit"  action="edit" 
                 visibleWhen="#{ editable == true  }"/>    

        <!-- buttons -->
        <invoker type="formActions" caption="Supersede"  action="supersede" index="0" visibleWhen="#{ overridable == true }"/>    
        <invoker type="formActions" caption="Close Issue"  action="closeIssue" index="1" visibleWhen="#{ overridable == true }"/>    
        
        <invoker type="formActions" caption="View History"  action="viewHist" index="2" visibleWhen="#{pagename == 'view' }"/>
        <invoker type="formActions" caption="Back"  action="back" index="2" visibleWhen="#{pagename == 'hist' }"/>
            
    </invokers>
    <code class="com.rameses.gov.etracs.obo.models.BuildingPermitRequirementModel" />
    <pages>
        <page template="com.rameses.gov.etracs.obo.views.BuildingPermitRequirementPage" />
    </pages>
</workunit>