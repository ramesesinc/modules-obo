<workunit  extends="com/rameses/seti2/templates/FormReport.xml"> 
    <invokers>
        
         <invoker type="vw_building_application:form:appActions" 
                caption="Print Order of Payment"
                reportId="building_application_assessment"
                reportName="com/rameses/gov/etracs/obo/reports/assessment.jasper"
                action="preview"
                allowPrint="#{ task.state.matches('billing|payment') }"
                visibleWhen="#{ task.state.matches('approval|billing|payment') }"/>

         <invoker type="building_application_assessment:preview" 
                caption="Preview Assessment"
                reportId="building_application_assessment"
                reportName="com/rameses/gov/etracs/obo/reports/assessment.jasper"
                action="preview"
                allowPrint="#{ false }"/>
         
        <invoker type="formActions" caption="Send Email" action="sendEmail" 
                visibleWhen="#{ task.state.matches('billing|payment') }"
                 fileid="building_application_assessment"/>            
    </invokers>
    <code class="com.rameses.gov.etracs.obo.models.OboApplicationPrintoutModel" />
</workunit>