<workunit schemaName="vw_building_application" processName="building_application" allowCreate="false">
    <invokers>
        <invoker type="vw_building_application:form:appActions" caption="Generate RPT Documents" 
                 action="generateDocs" index="0" target="process" 
                 visibleWhen="#{!entity.rptvalidated}"/>

        <!-- RPT ACTIONS -->   
        <invoker type="vw_building_application:form:appActions" 
                caption="View Tax Dec Documents"
                action="viewList" target="popup" index="1"
                 visibleWhen="#{entity.rptvalidated == true}"/>
        
        <invoker type="formActions" 
                caption="Close"
                action="_close" index="0"/>

        <invoker type="formActions" 
                caption="View Item"
                action="viewItem" index="1"/>

    </invokers>
    <code class="com.rameses.gov.etracs.obo.building.models.BuildingApplicationRptModel" />
    <pages>
        <page name="list" template="com.rameses.seti2.views.FormListPage" />
    </pages>    
</workunit>