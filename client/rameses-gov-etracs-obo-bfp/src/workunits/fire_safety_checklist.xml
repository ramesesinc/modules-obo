<workunit>
    <invokers>
        <invoker type="vw_building_evaluation:form:extActions" 
                 caption="Add Fire Safety Checklist" action="create" index="1"
                 visibleWhen="#{ entity.firesafetychecklist?.objid==null 
                    &amp;&amp; editable==true 
                    &amp;&amp; entity.typeid.toLowerCase() == 'fsec' }" target="popup" />
        
        <invoker type="vw_building_evaluation:form:extActions" 
                 caption="View Fire Safety Checklist" action="open" index="1"
                 visibleWhen="#{ entity.firesafetychecklist?.objid!=null 
                    &amp;&amp; entity.typeid.toLowerCase() == 'fsec' }" target="popup" />
                 
       <invoker type="formActions" caption="Close" action="_close" />
       <invoker type="formActions" caption="Print Report" action="printReport" index="2" />  
    </invokers> 
    <code class="com.rameses.gov.etracs.bfp.models.FireSafetyChecklistModel" />            
    <pages>
        <page name="default" template="com.rameses.gov.etracs.bfp.views.FireSafetyChecklistPage" />
    </pages>
</workunit>