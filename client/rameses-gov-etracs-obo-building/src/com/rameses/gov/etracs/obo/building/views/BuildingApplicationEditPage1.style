"entity.*|applicant.*" [ editable == true ] {
    enabled: true;
}

"entity.*|applicant.*" [ editable == false ] {
    enabled: false;
}

"entity.(appno|apptype|appdate|permit.*)" [ mode == "edit1" ] {
    visible: false;
}

"entity.(appno)" [ txnmode == "ONLINE" ] {
    visible: false;
}

"entity.(permit.*|receipt.*|amtpaid)" [ txnmode=="ONLINE" && mode=="create" ] {
    visible: false;
}