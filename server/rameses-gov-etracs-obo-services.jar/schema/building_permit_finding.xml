<schema>
	<element tablename="building_permit_finding">
		<field name="objid" primary="true" prefix="BLDPMTFND"/>
		<field name="appid" required="true"/>
		<field name="parentid" />
		<field name="rootid" required="true"/>

		<field name="particulars" />
		<field name="dtcreated" />
		<field name="createdby_objid" />
		<field name="createdby_name" />

		<field name="category_objid" />		

		<!-- 0=editable 1=noneditable must override -->
		<field name="state" />
		<field name="resolvedby_objid" />
		<field name="resolvedby_name" />
		<field name="dtresolved" />
		<field name="remarks" />
		<field name="supersederid" />

		<complex name="parent" ref="building_permit_section" jointype="many-to-one" includefields="typeid" required="false">
			<key field="parentid" target="objid"/>
		</complex>	

	</element>
</schema>