		<field name="parentid" />
		<field name="particulars" />
		<field name="dtcreated" />
		<field name="createdby_objid" />
		<field name="createdby_name" />

		<field name="category_objid" />		

		<!-- 0=editable 1=noneditable must override -->
		<field name="state" />
		<field name="resolvedby_objid" />
		<field name="resolvedby_name" />
