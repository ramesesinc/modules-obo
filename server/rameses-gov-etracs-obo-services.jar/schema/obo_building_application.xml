<schema adapter="obodb">
	<element tablename="obo_building_application">
		<field name="objid" prefix="OBOAPP" primary="true"/>
		<field name="acctid" />
		<field name="appno" />
		<field name="appdate" />
		<field name="apptype" />
		<field name="title" />		
		<field name="description" />
		<field name="owner_objid" />
		<field name="owner_name" />
		<field name="owner_address_text" />
		<field name="owner_ctcid" />

		<field name="scope" />

		<complex name="occupancytype" ref="obo_occupancy_type" jointype="many-to-one" required="true">
			<key field="occupancytypeid" target="objid" />
		</complex>	

		<field name="numunits" />
		<field name="floorarea" />
		<field name="estimatedcost" />
		<field name="projectcost" />
		<field name="dtproposedconstruction" />
		<field name="dtexpectedcompletion" />
		<field name="supervisorid" />
		<field name="taskid" />

		<complex name="subapplications" ref="obo_building_subapplication" jointype="one-to-many">
			<key field="objid" target="appid"/>
		</complex> 

	</element>
</schema>