<schema adapter="obodb">
	<element tablename="obo_building_application">
		<field name="objid" prefix="OBOAPP" primary="true"/>
		<field name="acctid" />
		<field name="appno" />
		<field name="appdate" />
		<field name="apptype" />
		<field name="title" />		
		<field name="description" />
		<field name="owner_objid" />
		<field name="owner_name" />
		<field name="owner_address_text" />
		<field name="owner_ctcid" />

		<complex name="worktype" ref="obo_work_type" jointype="many-to-one" required="true">
			<key field="worktypeid" target="objid" />
		</complex>	

		<complex name="bldgtype" ref="obo_building_type" jointype="many-to-one" required="true">
			<key field="bldgtypeid" target="objid" />
		</complex>	

		<field name="numunits" />
		<field name="floorarea" />
		<field name="totalfloorarea" />
		<field name="height" />

		<field name="estimatedcost" />
		<field name="projectcost" />
		<field name="dtproposedconstruction" />
		<field name="dtexpectedcompletion" />
		<field name="supervisorid" />
		<field name="taskid" />

		<complex name="sections" ref="obo_building_application_section" jointype="one-to-many">
			<key field="objid" target="appid"/>
		</complex> 

	</element>
</schema>