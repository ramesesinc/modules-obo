<schema adapter="obodb">
	<element tablename="obo_evaluation_type">
		<field name="objid" primary="true" caption="Name" visible="true"/>
		<field name="title" caption="Title" searchable="true"/>
		<field name="org_objid" />
		<field name="org_name" />
		<field name="activationstate" />
		<field name="sortindex" />
		<field name="role" />

		<field name="permitid" />
		<field name="allowassessment" />

		<complex name="permittype" ref="obo_permit_type" jointype="many-to-one" required="false">
			<key field="permitid" target="objid" />
		</complex>	

	</element>
</schema>
		