<schema adapter="obodb">
	<element tablename="obo_application_subproc_task">
		<field name="taskid" primary="true"  prefix="OBOSUBPROCTSK" visible="true"/>
		<complex name="ref" ref="obo_application_subproc" jointype="many-to-one" required="false"> 
			<key field="refid" target="objid"/>
		</complex>
		<field name="parentprocessid" />
		<field name="state" required="true"/>
		<field name="dtcreated" required="true"/>
		<field name="startdate" />
		<field name="enddate" />
		<field name="assignee_objid" />
		<field name="assignee_name" caption="Assigned To" />
		<field name="actor_objid" />
		<field name="actor_name" />
		<field name="message" />	
		<field name="prevtaskid" />	
	</element>
</schema>
		