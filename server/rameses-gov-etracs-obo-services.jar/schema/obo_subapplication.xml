<schema adapter="obodb">
	<element tablename="obo_subapplication">
		<field name="objid" primary="true" prefix="OBOSUBAPP"/>
		<field name="appid" />
		<field name="sectionid" />

		<field name="taskid" />

		<complex name="section" ref="obo_section" required="false" jointype="many-to-one">
			<key field="sectionid" target="objid" />
		</complex>	

		<complex name="app" ref="obo_application" required="false" jointype="many-to-one">
			<key field="appid" target="objid" />
		</complex>	

		<complex name="task" ref="obo_subapplication_task" required="false" jointype="many-to-one">
			<key field="taskid" target="taskid" />
		</complex>

	</element>
</schema>