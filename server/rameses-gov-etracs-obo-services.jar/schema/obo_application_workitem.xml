<schema adapter="obodb">
	<element tablename="obo_application_workitem">
		<field name="objid" primary="true" prefix="OBOAPPWRKITM"/>
		<field name="appid" required="true"/>
		<field name="typeid" required="true"/>
		<field name="subappid"/>
		<field name="taskid" />
		<complex name="task" ref="obo_application_workitem_task" required="false" jointype="many-to-one">
			<key field="taskid" target="taskid" />
		</complex>
	</element>
</schema>