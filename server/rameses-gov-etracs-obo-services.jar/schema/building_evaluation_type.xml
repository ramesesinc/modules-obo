<schema>
	<element tablename="building_evaluation_type">
		<field name="objid" primary="true"/>
		<field name="title" />
		<field name="type" />
		<field name="org_objid" />
		<field name="org_name" />
		<field name="sortindex" />
		<field name="required" />
		<field name="role" />
		<field name="activationstate" />
		<field name="optional" />
		<field name="requirefee" />
		<field name="permitid" />
		<field name="issuanceid" />

		<complex name="permit" ref="obo_permit_type" jointype="many-to-one" required="false">
			<key field="permitid" target="objid" />
		</complex>	
		
		<complex name="issuance" ref="obo_permit_type" jointype="many-to-one" required="false">
			<key field="issuanceid" target="objid" />
		</complex>	


	</element>
</schema>	

