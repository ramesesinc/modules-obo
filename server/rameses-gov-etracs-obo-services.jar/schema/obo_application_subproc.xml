<schema adapter="obodb">
	<element tablename="obo_application_subproc">
		<field name="objid" primary="true" prefix="OBOSUBPROC"/>
		<field name="refid" required="true"/>
		<field name="typeid" required="true"/>
		<field name="maintaskid" required="true"/>

		<field name="taskid" />

		<complex name="app" ref="obo_application" required="false" jointype="many-to-one">
			<key field="refid" target="objid" />
		</complex>	

		<complex name="type" ref="obo_requirement_type" required="false" jointype="many-to-one">
			<key field="typeid" target="objid" />
		</complex>	


		<complex name="task" ref="obo_application_subproc_task" required="false" jointype="many-to-one">
			<key field="taskid" target="taskid" />
		</complex>

	</element>
</schema>