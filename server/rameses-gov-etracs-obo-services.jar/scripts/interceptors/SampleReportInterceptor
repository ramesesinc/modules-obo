import com.rameses.annotations.*; 

class SampleReportInterceptor {
	
	@After(pattern="FormReportService.getData", eval="#{ args[0].reportid == 'sample_report' }")
	public void getSampleReport( def evt ) {
		def p = evt.args[0];
		def result = evt.result;
		def objid = p.parameters?.id;
		
		println "PARAMETERS IS " + p.parameters;

		def list = [
			[firstname: "Berserk", lastname: "Thor"],
			[firstname: "Infinity", lastname: "Stone"],			
		]

		result.data = list;
	}	


}