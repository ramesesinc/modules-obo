<schema adapter="obodb">
	<element tablename="vw_obo_building_subapplication">
		<field name="objid" primary="true"/>
		<field name="appid"/>
		<field name="appno"/>
		<field name="appdate"/>
		<field name="apptype"/>
		<field name="title"/>		
		<field name="description"/>
		<field name="owner_objid"/>
		<field name="owner_name"/>
		<field name="owner_address_text"/>
		<field name="owner_ctcid"/>
		
		<field name="occupancytype_division"/>
		<field name="occupancytype_group"/>
		<field name="numunits"/>
		<field name="floorarea"/>
		<field name="totalfloorarea"/>
		<field name="height"/>

		<field name="estimatedcost"/>
		<field name="projectcost"/>
		<field name="dtproposedconstruction"/>
		<field name="dtexpectedcompletion"/>
		<field name="supervisorid"/>
		<field name="activationstate"/>

		<field name="typetitle"/>
		<field name="typeid"/>

		<!-- TASK RELATED INFO -->
		<field name="taskid"/>
		<field name="task_state" />	
		<field name="task_fromdate"/>
		<field name="task_timediff" />
		<field name="task_startdate" />	
		<field name="task_enddate" />	
		<field name="task_assignee_objid" />	
		<field name="task_assignee_name" caption="Assign To"/>	
		<field name="task_actor_objid" />	
		<field name="task_actor_name" />

	</element>
</schema>	
