import com.rameses.annotations.*
import com.rameses.util.*
import com.rameses.common.*
import com.rameses.services.extended.*

class OboOnlineBillingService  {
    @ProxyMethod
    public def getBilling( def params ) {
        def bill = [
            appno: 'A-230943',
            project: "2 BEDROOM APARTMENT",
            amount: 700,
            items: [
                [account: [name: "BUILDING APPLICATION FEE"], amount: 500],
                [account: [name: "FIRE SAFETY FEE"], amount: 200],
            ]
        ]
        println 'bill => ' + bill;
        return bill;
    }
}
