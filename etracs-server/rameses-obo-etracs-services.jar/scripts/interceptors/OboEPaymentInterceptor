import com.rameses.annotations.*; 

class OboEPaymentInterceptor {

	@After(pattern="EORService.post", eval="#{args[0].txntype=='obo'}")
	public void afterPostPayment( def evt ) { 
		def o = evt.args[0];
		def res = evt.result;

		println "ARGUMENTS ------------->"
		if( o ) {
			o.each { k,v->
				println k+"="+v;
			}
		}

		println "RESULT ------------->"
		if(res) {
			res.each { k,v->
				println k+"="+v;
			}
		}
	} 
}