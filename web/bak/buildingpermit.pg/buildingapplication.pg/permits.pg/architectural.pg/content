<script>
  var controller = new function() {
    var self = this;
    var svc = Service.lookup("TestOboService", "default");

    this.entity;
    this.onload = function() {
      if (!self.entity) self.entity = {designprofessional: {}, supervisor: {}};
      if (!self.entity.designprofessional) self.entity.designprofessional = {objid: self.entity.designprofessionalid};
      if (!self.entity.supervisor) self.entity.supervisor = {objid: self.entity.supervisorid};
    }

    this.saveDesignProfessional = function() {
      if (!self.entity.designprofessional.objid) self.entity.professional.objid = self.entity.designprofessionalid;
      var params = {
        permitid: self.entity.objid,
        type: "design",
        professional: self.entity.designprofessional
      }
      self.entity.designprofessional = svc.savePermitProfessional( params );
      alert("Design Professional successfully saved.");
    }

    this.saveSupervisor = function() {
      if (!self.entity.supervisor.objid) self.entity.supervisor.objid = self.entity.supervisorid;
      var params = {
        permitid: self.entity.objid,
        type: "supervisor",
        professional: self.entity.supervisor
      }

});


</script>
