<h1>Design Professional</h1>
@box2(objectname: "entity.designprofessional")
<input type="button" r:context="${PAGE.context}" r:name="saveDesignProfessional" value="Save"/>
<br/><br/>
<h1>Supervisor</h1>
@box2(objectname: "entity.supervisor")
<input type="button" r:context="${PAGE.context}" r:name="saveSupervisor" value="Save"/>