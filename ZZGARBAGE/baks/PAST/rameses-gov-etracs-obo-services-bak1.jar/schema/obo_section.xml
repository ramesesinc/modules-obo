<schema>
	<element tablename="obo_section">
		<field name="objid" primary="true" caption="Name" visible="true"/>
		<field name="title" caption="Title" searchable="true"/>
		<field name="org_objid" />
		<field name="org_name" />
		<field name="sortindex" />
		<field name="role" />

		<field name="buildingpermitstate" />
		<field name="occupancypermitstate" />

		<!-- if optional=0 then it is always fired. if 1 depends only if there are associated ancillary permits -->
		<field name="optional" />
		<field name="ancillarytypeid" />

		<field name="requirefee" />
		<field name="issuepermit" />
		<field name="reportname" />

	</element>
</schema>
		

